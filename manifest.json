{
  "name": "Bookmark Auto-Organizer",
  "version": "0.1",
  "description": "organizes bookmarks, work in progress",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "bookmarks",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_action": "popup.html"
  },
  "manifest_version": 2
}
