{
    "name": "Bookmark auto organizer",
    "version": "0.5",
    "description": "Sorts Bookmarks from most used to least used",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "bookmarks",
        "storage",
        "tabs"
    ],
    "browser_actions": [
      "default_action": "popup.html"
    ],
    "manifest_version": 2


}
