{
  "name": "Bookmark Auto-Manager",
	"description": "Sorts your bookmarks from recently used to least used.",
  "version": "0.7",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "bookmarks",
		"storage",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
	"manifest_version": 2
}
