{
  "name": "histor.y",
	"description": "This extension sorts your bookmarks by most recently used.",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": { "128": "icon/Histor.y_Logo_128.png" },
  "permissions": [
    "bookmarks",
		"storage",
    "tabs"
  ],
  "browser_action": {

    "default_popup": "popup.html"
  },
  "manifest_version": 2

}
